# frozen_string_literal: true

source "https://rubygems.org"
# gemspec

if ENV["GH_PAGES"]
  gem "github-pages"
elsif ENV["JEKYLL_VERSION"]
  gem "jekyll", ENV["JEKYLL_VERSION"]
end

gem 'jekyll-feed'
gem 'jekyll-redirect-from'
gem 'jekyll-sitemap'
gem 'jekyll-archives'
gem 'jekyll-sass-converter', '~> 2.2'

gem "webrick", "~> 1.9"
